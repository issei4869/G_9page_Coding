<% project='.' ; ejsPath='.' 

let lists = [
    {subtitie: '全長' , text: '4.79 ～ 14 m'},
    {subtitie: '最大搭載重量' , text: '70 kg（下付け）'},
    {subtitie: '本体重量' , text: '1316 kg（ウェイト別）'},
    {subtitie: '最大レンズ高' , text: '12.19 m'},
    {subtitie: 'Max. Speed' , text: '2.6 m/s'}
]

%>


<div class="p-crane-item">
    <div class="p-crane-item__img">
        <img src="./assets/images/common/crane1.jpg" alt="クレーン" />
    </div>
    <div class="p-crane-item__content">
        <div class="p-crane-item__wrapper">
            <h3 class="p-crane-item__title"><%- title %></h3>
            <!-- prettier-ignore -->
            <%- include(project + ejsPath + '/component/_c-tab' ,{text:'単体レンタル可', color:''}) %>
        </div>
        <dl class="p-crane-item__list-items">
            <% lists.forEach(function(list) { %>
              <div class="p-crane-item__list-item">
                <dt><%- list.subtitie %></dt>
                <dd><%- list.text %></dd>
              </div>
            <% }); %>
        </dl>
    </div>
</div>
