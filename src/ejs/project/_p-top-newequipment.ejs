%>


<div class="p-top-newequipment">
  <div class="p-top-newequipment__inner l-inner">
    <div class="p-top-newequipment__heading">
      <%- include(ejsPath + './component/_c-section-heading', {
        page: "",
        heading_eng:'New Equipment',
        heading_ja:'機材新入荷のお知らせ',
        anima:''
      }) %>
    </div>
    <div class="p-top-newequipment__slick">
      <ul class="p-top-newequipment__list js-service-slick">
        <% cards.forEach(function(card) { %>
          <li class="p-top-newequipment__item">
            <%- include(project + '/_p-newequipment-card', {
              url: card.url,
              img: card.img,
              alt: card.alt,
              tab: card.tab,
              date: card.date,
              datetime: card.datetime,
              text: card.text
            }) %>
          </li>
        <% }); %>
      </ul>
      <div class="p-top-newequipment__arrow-wrap">
        <div class="p-top-newequipment__arrow p-top-newequipment__arrow--prev js-service-arrow-prev"></div>
        <div class="p-top-newequipment__arrow p-top-newequipment__arrow--next js-service-arrow-next"></div>
      </div>
      <div class="p-top-newequipment__btn">
        <%- include(project + './component/_c-btn2', { link: 'equipment.html', text: '機材一覧へ' }) %>
      </div>
    </div>
  </div>
