<%
project='.' ; ejsPath='.'

let items = [
  {
    img: "./assets/images/common/crane1.jpg",
    alt: "クレーン",
    title: 'GF-8',
    tabtext: '単体レンタル可',
    tabcolor: ''
  },
  {
    img: "",
    alt: "",
    title: 'GF-16',
    tabtext: '単体レンタル可',
    tabcolor: 'black'
  }
]
%>


<div class="p-sub-crane">
    <div class="p-sub-crane__inner">
        <aside class="archive-blog__sidebar">
            <%- include(ejsPath + './project/_p-sidebar-nav' ) %>
        </aside>
        <ul class="p-sub-crane__items">
            <% items.forEach(function(item) { %>
                <li class="p-sub-crane__item">
                    <%- include(project + '/_p-crane-item', {
                        img: item.img,
                        alt: item.alt,
                        title: item.title,
                        tabtext: item.tabtext,
                        tabcolor: item.tabcolor
                    }) %>
                </li>
            <% }); %>
        </ul>
    </div>
</div>